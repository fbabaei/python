#!/usr/local/bin/python

def createNewList(list=[]):
     i = 0
     newlist=[]
     for ii in list:
             i += 2
             if i is ii and list[ii]%2 == 0:
                     newlist.append(list[ii])
     return newlist
list = [1,2,3,4,5,6,7,8,9,12,14,15,16,18]
newl = createNewList(list)
print newl

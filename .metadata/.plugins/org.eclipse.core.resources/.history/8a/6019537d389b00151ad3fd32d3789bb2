def createNewList(list=[]):
    print list
    i = 0
    print (" Initial Value of i = %s ") % i
    newlist=[]
    for ii in list:
        print ("Value of i = %s ") % i
        i += 2
        print i
        if i == ii and list[ii]%2 == 0:
            print "for loop"
            newlist.appsnd(list[ii])
            print i
            print ii
            print newlist
        return newlist
l = [1,2,3,4,5,6,7,8,9,12,14,15,16,18]

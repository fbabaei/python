def word_frequency(words):
    '''
    returns frequency of words in a list of words
    '''
    frequency = {}
    for w in words:
        frequency[w] = frequency.get(w,0)+ 1
    return frequency

if __name__ == '__main__':
    words = ['a','b','v','f','d','a','v','g','d','s','c','g','h','e','s','a','b']
    print word_frequency(words)
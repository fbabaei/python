l = ['abc', 'cde', 'fed', 'cba', 'def', 'edf']

# l = [list(s)for s in l]
l2 = map(list, l)
l3 = []
for l in l2:
    l3.append(sorted(l))
n = len(l3)-1
l4 = []
print l3
while n > 0:
    while (n-1)> 0:
        if cmp(l3[n], l3[n - 1])== 0:
            l4.append(l3[n])
            del l3[n]
            n = len(l3) -1
    n = len(l3) -1 
          
if __name__ == '__main__':
    print "L4: %s " % l4
    print "L3: %s " % l3
l = [2,1,4,3,5,6,4]
x = iter(l)
print x
nl = []
for i in range(len(l)):
    nl.append(x.next())
#     print nl[::-1]
    ln = nl[::-1]

class zrange_iter():
    def __init__(self, n):
        self.n = n
        self.i = 0
    
    def __iter__(self):
        return self;

    def next(self):
        if self.i < self.n:
            i = self.i
            self.i += 1
            return i
        else:
            raise StopIteration()
class zrange:
    def __init__(self, n):
        self.n = n
    def __iter__(self):
        return zrange_iter(self.n)
'''
    generators 
'''
def yrange(n):
    i = 0
    while i < n: 
        yield i 
        i += 1   

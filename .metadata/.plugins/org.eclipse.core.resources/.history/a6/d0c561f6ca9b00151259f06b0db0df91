#!/usr/local/bin/python

def createNewList(l=[]):
    print l
    i = 0
    newlist=[]
    for ii in l:
#        if i == ii and list[sii]%2 == 0:
        print ("l[%s] = %s")% (ii, l[ii])
        if i == ii and l[ii]%2 == 0:
            print "if statement"
            newlist.append(l[ii])
            print newlist
        i += 2
    return newlist
l = [1,3,4,5,6,7,8,9,12,14,15,16,18]
# newl = createNewList(l)
# print newl
def get_evenlist(l=[]):
    return [i for i in l if not i%2]

l = [1,2,3,4,5,6,7,8,9]
# print get_evenlist(l)
l = [3,4,5,7,6,8,9,3,4,6,8,9]
def get_evenElements(l=[]):
    return [l[1::2]]
print("even elements list:") % get_evenElements(l)

evl = get_evenElements(l)
print ("final list: %s ") % get_evenlist(evl)   
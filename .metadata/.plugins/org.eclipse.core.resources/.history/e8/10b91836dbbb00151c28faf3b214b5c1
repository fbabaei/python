l = ['abc', 'cde', 'fed', 'cba', 'def', 'edf']

# l = [list(s)for s in l]
l2 = map(list, l)
l3 = []
l4 = []
for l in l2:
    l3.append(''.join(sorted(l)))
        
l4 = []
n = len(l3)-1

          
if __name__ == '__main__':
    print "L3: %s " % l3    
    print "L4: %s " % l4
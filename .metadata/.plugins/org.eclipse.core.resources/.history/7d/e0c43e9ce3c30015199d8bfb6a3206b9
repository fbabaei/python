print [x for x in range(1,10) if x%2 == 0]

def f(i):
    if i%3==0 and i%5== 0: 
        print 'Euclid','Euler'
    elif i%3 == 0:
        print 'Euclid'
    elif i%5 == 0:
        print 'Euler'
    else:
        print i
        
if __name__ == '__main__':
    f(12500)
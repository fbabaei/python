def get_evenlist(l=[]):
    return [i for i in l if not i%2]

l = [1,2,3,4,5,6,7,8,9]
# print get_evenlist(l)
l = [3,4,5,7,6,8,9,3,4,6,8,9]
def get_evenval_of_evenele(l=[]):
    l2 = get_evenlist(l)
    print ("L2 list = %s") % l2
    return [i for i in l2 if not l2[i]%2]
print get_evenval_of_evenele(l)
    
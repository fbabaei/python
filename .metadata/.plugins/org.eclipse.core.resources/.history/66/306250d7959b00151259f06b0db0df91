def createNewList(l=[]):
    print l
    i = 0
    newlist=[]
    for ii in l:
#        if i == ii and list[sii]%2 == 0:
        print ("l[%s] = %s")% (ii, l[ii])
        if i == ii and l[ii]%2 == 0:
            print "if statement"
            newlist.append(l[ii])
            print newlist
        i += 2
    return newlist
l = [1,3,4,5,6,7,8,9,12,14,15,16,18]
